    "@types/helmet": "^4.0.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.151",
    "@types/mongodb": "^3.5.18",
    "@types/node": "^13.13.35",
    "analytics-node": "^3.3.0",
    "axios": "^0.18.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "flatted": "^2.0.2",
